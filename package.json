{
  "name": "import-codesign-certs",
  "version": "1.0.3",
  "private": true,
  "description": "GitHub Action for Importing Code-signing Certificates into a Keychain",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apple-Actions/import-codesign-certs.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Itty Bitty Apps Ptr Ltd",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/parser": "^5.59.7",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.41.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
